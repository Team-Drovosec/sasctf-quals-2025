+--- /dev/null
++++ b/customsyscalls/customsyscalls.c
+@@ -0,0 +1,26 @@
++#include <linux/kernel.h>
++#include <linux/syscalls.h>
++
++const unsigned char XOR_KEY[8] = {0x2a, 0x99, 0x6e, 0xb0, 0x2c, 0x37, 0x2e, 0x8b};
++const unsigned char encrypted[8] = {0x4f, 0xf4, 0x1b, 0xdc, 0x4d, 0x43, 0x41, 0xf9};
++
++static inline void decrypt(unsigned char *enc, unsigned char *data, unsigned long size) {
++    for (unsigned long i = 0; i < size; ++i) {
++        data[i] = enc[i] ^ XOR_KEY[i];
++    }
++}
++
++SYSCALL_DEFINE2(suscall, unsigned char __user *, data, unsigned long, size)
++{   
++    if (size != 8) { 
++        return -EINVAL;
++    }
++
++    unsigned char out[8 + 1] = {0};
++    decrypt(encrypted, out, 8);
++    printk("SUSCALL");
++
++    // printk("suscall");
++    // printk(out);
++    return copy_to_user(data, out, size);
++}
